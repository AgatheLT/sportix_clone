<div class="container">
  <%= simple_form_for @sport_session do |f| %>
    <%= f.input :sport %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :location %>
    <%= f.input :start_time %>
    <%= f.input :duration %>
    <%= f.input :price %>
    <%= f.input :capacity %>
    <%= f.input :user_id, :as => :hidden %>
    <%= f.button :submit, class: 'btn btn-primary' %>
  <% end %>
</div>
  